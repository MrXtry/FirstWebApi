@model IEnumerable<FirstWebApi.Models.Students>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentRollNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentDepartment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentbatchNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsPresent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StudentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentRollNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentDepartment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentbatchNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsPresent)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
